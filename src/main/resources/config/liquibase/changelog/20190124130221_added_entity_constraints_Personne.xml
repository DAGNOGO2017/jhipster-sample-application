<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Personne.
    -->
    <changeSet id="20190124130221-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="personnes_id"
                                 baseTableName="personne_activite"
                                 constraintName="fk_personne_activite_personnes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="personne"/>
        <addForeignKeyConstraint baseColumnNames="activites_id"
                                 baseTableName="personne_activite"
                                 constraintName="fk_personne_activite_activites_id"
                                 referencedColumnNames="id"
                                 referencedTableName="activite"/>
        
        <addForeignKeyConstraint baseColumnNames="personnes_id"
                                 baseTableName="personne_petition"
                                 constraintName="fk_personne_petition_personnes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="personne"/>
        <addForeignKeyConstraint baseColumnNames="petitions_id"
                                 baseTableName="personne_petition"
                                 constraintName="fk_personne_petition_petitions_id"
                                 referencedColumnNames="id"
                                 referencedTableName="petition"/>
        
    </changeSet>
</databaseChangeLog>
